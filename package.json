{
  "name": "heirloom-seed",
  "version": "1.0.0",
  "private": true,
  "description": "我的模版工程",
  "keywords": [
    "ES6",
    "webpack",
    "react",
    "redux"
  ],
  "scripts": {
    "test": "jest",
    "prestart": "rm -rf public && pm2 kill",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "stop": "pm2 stop www",
    "prestart:prod": "npm run build",
    "start:prod": "pm2 start bin/www && pm2 logs",
    "start:dev": "webpack-dev-server --hot --progress --colors --inline --content-base public/ --history-api-fallback",
    "build": "rm -rf public && webpack",
    "predeploy": "npm test -- --no-cache --coverage",
    "deploy": "npm run build",
    "postdeploy": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyuanxiang/heirloom-seed.git"
  },
  "author": "Xu Yuanxiang<xuyuanxiang@wosai-inc.com>",
  "bugs": {
    "url": "https://github.com/xuyuanxiang/heirloom-seed/issues"
  },
  "aliyun": {
    "region": "",
    "accessKeyId": "",
    "accessKeySecret": "",
    "bucket": ""
  },
  "homepage": "https://github.com/xuyuanxiang/heirloom-seed#readme",
