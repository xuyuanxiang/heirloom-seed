    "aliyunoss-webpack-plugin": "^0.2.0",
    "autoprefixer": "^6.5.0",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "express-useragent": "^1.0.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "if-env": "^1.0.0",
    "json-loader": "^0.5.4",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-scss": "^0.3.0",
    "precss": "^1.4.0",
    "pm2": "^2.0.19",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.8",
    "url-loader": "^0.5.7",
