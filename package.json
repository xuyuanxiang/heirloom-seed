{
  "name": "heirloom-seed",
  "version": "1.0.0",
  "private": true,
  "description": "我的模版工程",
  "keywords": [
    "ES6",
    "webpack",
    "react",
    "redux"
  ],
  "scripts": {
    "test": "jest",
    "start": "NODE_ENV=development webpack-dev-server --hot --progress --colors --inline --content-base public/ --history-api-fallback",
    "build": "NODE_ENV=production BABEL_ENV=release webpack",
    "predeploy": "NODE_ENV=production npm test -- --no-cache --coverage",
    "deploy": "npm run build",
    "postdeploy": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "preserve": "NODE_ENV=production npm install --registry=https://registry.npm.taobao.org",
    "serve": "NODE_ENV=production node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyuanxiang/heirloom-seed.git"
  },
  "author": "Xu Yuanxiang<xuyuanxiang@wosai-inc.com>",
  "bugs": {
    "url": "https://github.com/xuyuanxiang/heirloom-seed/issues"
  },
  "homepage": "https://github.com/xuyuanxiang/heirloom-seed#readme",
  "devDependencies": {
    "aliyunoss-webpack-plugin": "^0.2.0",
    "autoprefixer": "^6.5.0",
