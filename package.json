    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.6",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
